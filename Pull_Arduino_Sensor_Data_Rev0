import serial

arduino_data = serial.Serial('COM3', 115200, timeout = 0.05)  # establish serial communication link

def get_arduino_sensor_data():
    run = 1
    while run == 1:
        sensor_data = arduino_data.readline().decode('utf-8')[:-2]
        if sensor_data != '':
            run = 0
    return sensor_data

data = get_arduino_sensor_data()
print(data)
